{
	"name": "baukasten/blocks-preview",
	"description": "A preview functionality plugin for the Baukasten Kirby CMS that enables real-time preview of content blocks during editing.",
	"type": "kirby-plugin",
	"license": "MIT",
	"version": "1.0.0",
	"authors": [
		{
			"name": "Baukasten Team",
			"email": "contact@matthiashacksteiner.net"
		}
	],
	"require": {
		"getkirby/composer-installer": "^1.2",
		"getkirby/cms": "^3.7"
	},
	"extra": {
		"installer-name": "baukasten-blocks-preview"
	},
	"config": {
		"optimize-autoloader": true
	},
	"keywords": [
		"kirby",
		"cms",
		"preview",
		"blocks",
		"baukasten",
		"realtime",
		"editing"
	]
}